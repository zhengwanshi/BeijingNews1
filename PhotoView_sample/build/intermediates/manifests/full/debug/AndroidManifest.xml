<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uk.co.senab.photoview.sample"
    android:versionCode="100"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.INTERNET" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="com.github.chrisbanes.photoview.sample.LauncherActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.github.chrisbanes.photoview.sample.SimpleSampleActivity" />
        <activity android:name="com.github.chrisbanes.photoview.sample.ViewPagerActivity" />
        <activity android:name="com.github.chrisbanes.photoview.sample.RotationSampleActivity" />
        <activity android:name="com.github.chrisbanes.photoview.sample.PicassoSampleActivity" />
        <activity android:name="com.github.chrisbanes.photoview.sample.ActivityTransitionActivity" />
        <activity android:name="com.github.chrisbanes.photoview.sample.ActivityTransitionToActivity" />
        <activity android:name="com.github.chrisbanes.photoview.sample.ImmersiveActivity" />
    </application>

</manifest>